<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_second"
    app:startDestination="@id/EmptyDetailsFragment">

    <fragment
        android:id="@+id/EmptyDetailsFragment"
        android:name="js.task.views.EmptyDetailsFragment"
        android:label="@string/details_empty_fragment_label"
        tools:layout="@layout/fragment_details_empty">

        <action
            android:id="@+id/action_to_DetailsFragment"
            app:destination="@id/DetailsFragment" />

    </fragment>

    <fragment
        android:id="@+id/DetailsFragment"
        android:name="js.task.views.DetailsFragment"
        android:label="@string/details_fragment_label"
        tools:layout="@layout/fragment_details">

        <action
            android:id="@+id/action_to_DetailsFragment"
            app:destination="@id/DetailsFragment" />

    </fragment>

</navigation>